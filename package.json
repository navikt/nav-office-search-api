    "node-cache": "5.1.2",
    "node-schedule": "2.1.0",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@octokit/core": "4.1.0",
    "@types/express": "4.17.15",
    "@types/jsonwebtoken": "9.0.0",
    "@types/node": "18.11.18",
    "@types/node-schedule": "2.1.0",
    "@types/uuid": "9.0.0",
    "dotenv": "16.0.3",
    "eslint": "8.31.0",
    "prettier": "2.8.2",
    "typescript": "4.9.4"
  }
